{
	"name": "basadify",
	"displayName": "Basadify",
	"author": {
		"name": "Hezy Laplacian",
		"url": "https://www.facebook.com/HezyLaplacian/"
	},
	"publisher": "hezylaplacian",
	"description": "Give your code some help from the Lord",
	"repository": {
		"type": "git",
		"url": "https://github.com/hezylaplacian/basadify.git"
	},
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#45045e",
		"theme": "dark"
	},
	"version": "1.0.5",
	"license": "SEE LICENSE FILE",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:basadify.basadify"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "basadify.basadify",
				"title": "Basadify - May the Lord help your code üôèüèª"
			}
		],
		"keybindings": [
			{
				"command": "basadify.basadify",
				"key": "ctrl+b ctrl+s",
				"mac": "cmd+b cmd+s",
				"linux": "ctrl+b ctrl+s",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
